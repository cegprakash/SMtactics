{
  "name": "Striker Manager Tactics Manager",
  "version": "0.2.1",
  "manifest_version": 2,
  "description": "Striker Manager Tactics manager",
  "content_scripts": [
    {
      "matches" : ["http://*.strikermanager.com/alineacion.php*"],
      "all_frames" : true,
      "include_globs" : ["http://en*.strikermanager.com*"],
      "js":["remote.js","player.js","tactic.js","advtactic.js","lineup.js","shooters.js","storage.js","popup.js"]
    }
  ],
  "permissions": [
    "http://en3.strikermanager.com/",
    "http://en2.strikermanager.com/",
    "http://en1.strikermanager.com/",
    "http://en.strikermanager.com/",
    "storage",
    "tabs"
  ]
}